              "Commands": [
                {
                  "SemanticQueryDataShapeCommand": {
                    "Query": {
                      "Select": [
                        { "Column": { "Expression": { "SourceRef": { "Source": "k" } }, "Property": "UniqueUsers" }, "Name": "KPISummary.UniqueUsers" }
