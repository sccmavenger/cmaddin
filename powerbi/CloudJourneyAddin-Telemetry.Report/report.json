            "title": "Total Errors (7d)"
          }
        },
        {
          "id": "chart_dau",
          "position": { "x": 20, "y": 160, "z": 2000, "width": 600, "height": 280 },
          "visual": {
            "visualType": "lineChart",
            "query": {
              "Commands": [
                {
                  "SemanticQueryDataShapeCommand": {
                    "Query": {
                      "Select": [
                        { "Column": { "Expression": { "SourceRef": { "Source": "a" } }, "Property": "Date" }, "Name": "AppLaunches.Date" },
                        { "Column": { "Expression": { "SourceRef": { "Source": "a" } }, "Property": "UniqueUsers" }, "Name": "AppLaunches.UniqueUsers" }
                      ],
                      "From": [{ "Name": "a", "Entity": "AppLaunches", "Type": 0 }],
                      "OrderBy": [{ "Direction": 1, "Expression": { "Column": { "Expression": { "SourceRef": { "Source": "a" } }, "Property": "Date" } } }]
                    }
                  }
                }
              ]
            },
            "title": "Daily Active Users (90 Days)"
          }
        },
        {
          "id": "chart_versions",
          "position": { "x": 640, "y": 160, "z": 2001, "width": 300, "height": 280 },
          "visual": {
            "visualType": "donutChart",
            "query": {
              "Commands": [
                {
                  "SemanticQueryDataShapeCommand": {
                    "Query": {
                      "Select": [
                        { "Column": { "Expression": { "SourceRef": { "Source": "v" } }, "Property": "Version" }, "Name": "VersionDistribution.Version" },
                        { "Column": { "Expression": { "SourceRef": { "Source": "v" } }, "Property": "Users" }, "Name": "VersionDistribution.Users" }
                      ],
                      "From": [{ "Name": "v", "Entity": "VersionDistribution", "Type": 0 }]
                    }
                  }
                }
              ]
            },
            "title": "Version Distribution"
          }
        },
        {
          "id": "chart_errors",
          "position": { "x": 960, "y": 160, "z": 2002, "width": 300, "height": 280 },
          "visual": {
            "visualType": "areaChart",
            "query": {
              "Commands": [
                {
                  "SemanticQueryDataShapeCommand": {
                    "Query": {
                      "Select": [
                        { "Column": { "Expression": { "SourceRef": { "Source": "e" } }, "Property": "Date" }, "Name": "DailyErrorTrend.Date" },
                        { "Column": { "Expression": { "SourceRef": { "Source": "e" } }, "Property": "Errors" }, "Name": "DailyErrorTrend.Errors" }
                      ],
                      "From": [{ "Name": "e", "Entity": "DailyErrorTrend", "Type": 0 }],
                      "OrderBy": [{ "Direction": 1, "Expression": { "Column": { "Expression": { "SourceRef": { "Source": "e" } }, "Property": "Date" } } }]
                    }
                  }
                }
              ]
            },
            "title": "Error Trend (90 Days)"
