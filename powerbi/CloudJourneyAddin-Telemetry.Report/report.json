        {
          "displayName": "Trends",
          "position": { "x": 20, "y": 160, "width": 1240, "height": 540 }
        }
      ],
      "visualContainers": [
        {
          "id": "kpi_launches",
          "position": { "x": 20, "y": 20, "z": 1000, "width": 200, "height": 120 },
          "visual": {
            "visualType": "card",
            "query": {
              "Commands": [
                {
                  "SemanticQueryDataShapeCommand": {
                    "Query": {
                      "Select": [
                        { "Column": { "Expression": { "SourceRef": { "Source": "k" } }, "Property": "TotalLaunches" }, "Name": "KPISummary.TotalLaunches" }
                      ],
                      "From": [{ "Name": "k", "Entity": "KPISummary", "Type": 0 }]
                    }
                  }
                }
              ]
            },
            "title": "Total Launches (7d)"
          }
        },
        {
          "id": "kpi_users",
          "position": { "x": 240, "y": 20, "z": 1001, "width": 200, "height": 120 },
