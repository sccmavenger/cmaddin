        },
        {
          "id": "chart_wau",
          "position": { "x": 20, "y": 460, "z": 2003, "width": 600, "height": 240 },
          "visual": {
            "visualType": "lineChart",
            "query": {
              "Commands": [
                {
                  "SemanticQueryDataShapeCommand": {
                    "Query": {
                      "Select": [
                        { "Column": { "Expression": { "SourceRef": { "Source": "w" } }, "Property": "WeekStart" }, "Name": "WeeklyActiveUsers.WeekStart" },
                        { "Column": { "Expression": { "SourceRef": { "Source": "w" } }, "Property": "WAU" }, "Name": "WeeklyActiveUsers.WAU" }
                      ],
                      "From": [{ "Name": "w", "Entity": "WeeklyActiveUsers", "Type": 0 }],
                      "OrderBy": [{ "Direction": 1, "Expression": { "Column": { "Expression": { "SourceRef": { "Source": "w" } }, "Property": "WeekStart" } } }]
                    }
                  }
                }
              ]
            },
            "title": "Weekly Active Users (90 Days)"
          }
        },
        {
          "id": "table_topfeatures",
          "position": { "x": 640, "y": 460, "z": 2004, "width": 620, "height": 240 },
          "visual": {
            "visualType": "tableEx",
            "query": {
              "Commands": [
                {
                  "SemanticQueryDataShapeCommand": {
                    "Query": {
                      "Select": [
                        { "Column": { "Expression": { "SourceRef": { "Source": "f" } }, "Property": "FeatureName" }, "Name": "FeatureUsage.FeatureName" },
                        { "Column": { "Expression": { "SourceRef": { "Source": "f" } }, "Property": "UsageCount" }, "Name": "FeatureUsage.UsageCount" },
                        { "Column": { "Expression": { "SourceRef": { "Source": "f" } }, "Property": "UniqueUsers" }, "Name": "FeatureUsage.UniqueUsers" }
                      ],
                      "From": [{ "Name": "f", "Entity": "FeatureUsage", "Type": 0 }],
                      "OrderBy": [{ "Direction": 2, "Expression": { "Column": { "Expression": { "SourceRef": { "Source": "f" } }, "Property": "UsageCount" } } }],
                      "Top": 10
                    }
                  }
                }
